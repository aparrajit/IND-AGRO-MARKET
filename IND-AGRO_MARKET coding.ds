											percentage_available = 100 - input.Contract_Info.Percentage_collected;
										}
										else
										{
											percentage_available = 100;
										}
										input.Percentage_of_Contribution = percentage_available;
										if(Contract_Info.Contract_Status == "Insufficient Funds" || (input.Contract_Info.Maximum_Number_of_contributors - input.Contract_Info.Total_Contributors == 1 && !input.Self_funded))
