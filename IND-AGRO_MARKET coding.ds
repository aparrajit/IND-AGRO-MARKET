			(
				Harvest_Date as "Tentative Harvest Date"
				Contract_Kart as "Contract Kart"
				(
					view = All_Contract_Karts
				)
				Investigator_Approval as "Harvest Investigator Approval"
				custom action "Perform Harvest"
				(
	  			workflow = perform_harvest
				show action in edit record menu = true
				column header = "Perform Harvest"
				condition = ((Status == "Ongoing" && Add_Contract.Investiator_approval_required_for_harvest == "No" && Harvest_Date in next 7 days) || (Add_Contract.Investiator_approval_required_for_harvest == "Yes" && Harvest_Investigator.ID is null && Harvest_Date in next 7 days && Status == "Ongoing"))
				)
				Status
				Number
				Actual_Date as "Actual Date"
				Harvest_Investigator as "Harvest Investigator"
				Harvest_Investigator.Name  as "Supervisor Name"
	 			(
	      			displayformat = [first_name+""+prefix+""+last_name+""+suffix]
	 			)
				Harvest_Investigator.Phone_Number as "Supervisor Phone"
				(
					displayformat = plainnumber 
					linktodial = enable
				)
				ID
			)
			options
			(
				icon = "travel-world"
	 		)
			conditional formatting
			(
				"New Formatting"
				{
				condition = (Status == "Ongoing")
				fields = [Harvest_Date]
				format = "color:#ffffff;background-color:#e84c3d;"
				}
				"New Formatting"
				{
				condition = (Status == "Completed")
				fields = [Harvest_Date]
				format = "color:#000000;background-color:#1bbc9b;"
				}
			)
			quickview
			(
				layout
				(
		 			type = -1
					datablock1
					(
			 			layout type = -1
						fields
						(
							Harvest_Date as "Tentative Harvest Date"
							Investigator_Approval as "Harvest Investigator Approval"
							Status
							Number
							Actual_Date as "Actual Date"
							Harvest_Investigator as "Harvest Investigator"
						)
					)
				)

				menu
    			(
    	 			header
    	 			(
    	 			)
    			)
    			action
    			(
							on click
							(
								View Record   	   
    						)
     			)
			)
			detailview
			(
				layout
				(
		 			type = 1
					datablock1
					(
			 			layout type = -2
						fields
						(
							Harvest_Date as "Tentative Harvest Date"
							Contract_Kart as "Contract Kart"
							Investigator_Approval as "Harvest Investigator Approval"
							Status
				"Perform Harvest"
							Harvest_Investigator.Name as "Supervisor Name"
							Harvest_Investigator.Phone_Number as "Supervisor Phone"
						)
					)
				)

				menu
    			(
    	 			header
    	 			(
    		 			Edit 
    	 			)
    			)
			)
