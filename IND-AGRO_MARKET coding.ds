			On_submit7 as "On submit"
			{
				type =  form
				form = Item_Summery
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						pro_rec = Products[ID == input.Item_ID.toLong()];
										farm_rec = Farm_Details[Farm_ID = input.Farm_ID];
										Unsold_Units = pro_rec.Unsold_Units;
										if(Quantity_required < Minimum_Order_Quantity && Unsold_Units > Minimum_Order_Quantity)
										{
											alert "Minimum unit of purchase should be " + input.Minimum_Order_Quantity;
										}
										else if(Quantity_required > Unsold_Units)
										{
											alert "Only " + Unsold_Units + " Units are available";
										}
										else
										{
											all_pending_orders = Kart[Kart_User_ID == zoho.loginuser && Order_Status == "Payment Pending" && Farm_Details.Farm_ID == input.Farm_ID] sort by Added_Time desc;
											new_kart = true;
											item_present = false;
											harvested_kart_id = 0;
											for each  y in all_pending_orders
											{
												for each  x in y.Order_Summery
												{
													if(!x.Products.isEmpty() && x.Products.Harvested == true && pro_rec.Harvested)
													{
														harvested_kart_id = y.ID;
														new_kart = false;
													}
													if(x.Product_Name == input.Item_Name)
													{
														item_present = true;
													}
												}
												if(!new_kart)
												{
													break;
												}
											}
											if(!item_present)
											{
												if(!new_kart)
												{
													pending_kart = Kart[ID == harvested_kart_id];
													row1 = Kart.Order_Summery();
													row1.Cost_of_Purchase=input.Cost_of_Purchase;
													row1.Cost_per_unit=input.Cost_per_unit;
													row1.Quantity_of_purchase=input.Quantity_required;
													row1.Unit_of_Measurement=pro_rec.Unit_of_Measurement;
													row1.Product_Name=input.Item_Name;
													row1.Products=input.Item_ID;
													pending_kart.Order_Summery.insert(row1);
													zoho.pushNotification(zoho.loginuserid,"Item added to Kart","Item added to existing Kart with the farm " + farm_rec.Farm_name + ". Build your kart by addin addding items frm the same farm. Check out kart to place your order to this farm.","My_Kart",x.ID);
													openUrl("#Script:dialog.close","same window");
												}
												else
												{
													personal_details = Personal_Details[Added_User == zoho.loginuser];
													preferred_railway = personal_details.Nearest_RMS_station;
													preferred_address = personal_details.Personal_Address;
													for each  other_addr in personal_details.Other_Addresses_for_shipment
													{
														if(other_addr.Preferred_address_for_delivery == true)
														{
															preferred_railway = other_addr.Nearest_Railway_Station_code;
															preferred_address = other_addr.Address;
															break;
														}
													}
													row1 = Kart.Order_Summery();
													row1.Cost_of_Purchase=input.Cost_of_Purchase;
													row1.Cost_per_unit=input.Cost_per_unit;
													row1.Quantity_of_purchase=input.Quantity_required;
													row1.Unit_of_Measurement=pro_rec.Unit_of_Measurement;
													row1.Product_Name=input.Item_Name;
													row1.Products=input.Item_ID;
													rows = Collection();
													rows.insert(row1);
													x = insert into Kart
										[
											Added_User=zoho.loginuser
											Kart_User_ID=zoho.loginuser
											Order_Status="Payment Pending"
											Farm_Details=farm_rec.ID
											Railwaystation_for_delivery=preferred_railway
											Address=preferred_address
											Order_Summery=rows
										];
													alert "New Kart created.";
													openUrl("#Script:dialog.close","same window");
												}
											}
											else
											{
												alert "Item already present in kart";
											}
										}
					)
					}
				}
			}
