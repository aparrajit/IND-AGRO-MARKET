			join_community as "join community"
			{
				type =  functions
				form = Community
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						my_address_list = List();
										parent_map = Map();
										my_rec = Personal_Details[login_user == zoho.loginuser];
										if(my_rec.count() > 0)
										{
											addr = my_rec.Personal_Address;
											railwy_station = my_rec.Nearest_RMS_station;
											community_lat = input.Address.latitude.toLong();
											community_lon = input.Address.longitude.toLong();
											allowed = false;
											if(thisapp.getDistanceBetweenlatLong(community_lat,community_lon,addr.latitude.toLong(),addr.longitude.toLong()) < 3)
											{
												allowed = true;
											}
											else
											{
												for each  x in my_rec.Other_Addresses_for_shipment
												{
													lat2 = x.Address.latitude.toLong();
													lon2 = x.Address.longitude.toLong();
													dist = thisapp.getDistanceBetweenlatLong(community_lat,community_lon,lat2,lon2);
													if(dist < 3)
													{
														allowed = true;
														addr = x.Address;
														railwy_station = x.Nearest_Railway_Station_code;
														break;
													}
												}
											}
											if(allowed)
											{
												row1 = Community.Community_Members();
												row1.Personal_Details=my_rec.ID;
												row1.User_ID=zoho.loginuser;
												row1.Nearest_railwaystation=railwy_station;
												row1.Address=addr;
												input.Community_Members.insert(row1);
												if(Community_Member_Ids.isEmpty())
												{
													input.Community_Member_Ids = zoho.loginuser;
												}
												else
												{
													input.Community_Member_Ids = input.Community_Member_Ids + "," + zoho.loginuser;
												}
											}
											else
											{
												openUrl("#Page:Error?error=You dont have any address in your Personal details near this Community. Any of your address should be within 3 kilometers of this community.","popup window","height=auto,width=auto");
											}
										}
										else
										{
											openUrl("#Report:My_Personal_Details","same window");
										}
					)
					}
				}
			}
