		custom My_Farm_Products
		{
			displayName = "My Farm Products"
			show all rows from Products  [Added_User == zoho.loginuser]  
			(
				Cost_Unit as "Cost/Unit"
				Item_Stock as "Item Stock"
				Quantity_sold as "Quantity Sold"
				Image
				Unsold_Units as "Unsold Units"
				custom action "Update Stock Quantity"
				(
	  			workflow = update_stock
				show action in edit record menu = true
				column header = "Update Stock Quantity"
				condition = (Expired == false)
				)
				custom action "Hide Product From Store"
				(
	  			workflow = Hide_Products
				show action in edit record menu = true
				column header = "Hide Product From Store"
				condition = (Show_to_customers == true && Expired == false)
				)
				custom action "Show Product in store"
				(
	  			workflow = show_products
				show action in edit record menu = true
				column header = "Show Product in store"
				condition = (Show_to_customers == false && Expired == false)
				)
				custom action "Update Price"
				(
	  			workflow = update_price
				show action in edit record menu = true
				column header = "Update Price"
				condition = (Expired == false || Unsold_Units > 0)
				confirmation before execution
				(
					message for individual record = "Do you want to change the price for this product?"
					confirm button text = "Yes"		
					cancel button text = "No"
				)
				)
				custom action "Update Harvest and Expiry Dates"
				(
	  			workflow = update_harvest_expiry
				show action in edit record menu = true
				column header = "Update Harvest and Expiry Dates"
				condition = (Expired == false && Unsold_Units > 0)
				)
				Q_A.Add_Contract as "Q&A"
				(
					view = My_Customer_Questions
				)
				Product_Discription as "Product Description"
				Unit_of_Measurement as "Unit of Measurement"
				custom action "Allow Commuity Purchases"
				(
	  			workflow = Allow_community_Purchases
				show action in edit record menu = true
				column header = "Allow Commuity Purchases"
				condition = (Allow_Community_Purchases == "No")
				)
				custom action "Disallow Community Purchases"
				(
	  			workflow = disallow_purchase
				show action in edit record menu = true
				column header = "Disallow Community Purchases"
				condition = (Allow_Community_Purchases == "Yes")
				)
				Crop.Crop_Name as "Crop Name"
				Product_Quality as "Product Quality"
				Minimum_Order_Quantity as "Minimum Order Quantity"
				Date_of_Harvest as "Date of Harvest"
				custom action "Delete Product"
				(
	  			workflow = delete_product
				show action in edit record menu = true
				column header = "Delete Product"
				confirmation before execution
				(
					message for individual record = "Are you sure you want to delete this product?"
					confirm button text = "Yes"		
					cancel button text = "No"
				)
				)
				Expiry_date as "Expiry date"
				Community_order_unit as "Community order unit"
				Minimum_Community_Order_Quantity as "Minimum Community Order Quantity"
				custom action "Join/Create a Combo Pack"
				(
	  			workflow = create_a_combo_pack
				show action in edit record menu = true
				column header = "Join/Create a Combo Pack"
				condition = (Allow_Community_Purchases == "Yes" && Show_to_customers == true && Insuffecient_stock == false)
				)
				Display_Name as "Display Name"
			)
			filters
