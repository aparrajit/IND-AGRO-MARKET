				 float getDistanceBetweenlatLong(float lat1, float lon1, float lat2, float lon2)
         {
         	R = 6371;
         	// Radius of the earth in km
         	dLat = thisapp.d2grad(lat2 - lat1);
         	// deg2rad below
         	dLon = thisapp.d2grad(lon2 - lon1);
         	a = sin(dLat / 2) * sin(dLat / 2) + cos(thisapp.d2grad(lat1)) * cos(thisapp.d2grad(lat2)) * sin(dLon / 2) * sin(dLon / 2);
         	x = sqrt(a);
         	y = sqrt(1 - a);
         	z_json_stirng = thisapp.atan2("" + x,"" + y);
         	info z_json_stirng.get("output");
         	z = z_json_stirng.get("output").toDecimal();
         	c = 2 * z;
         	d = R * c;
         	// Distance in km
         	return d;
         }
