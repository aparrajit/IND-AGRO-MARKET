			contract_harvested as "contract_harvested"
			{
				type =  functions
				form = Add_Contract
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						kart_list = List();
										for each  contractor_rec in input.Contractor_Info
										{
											personal_details = Personal_Details[ID == contractor_rec.Personal_Details];
											preferred_railway = personal_details.Nearest_RMS_station;
											preferred_address = personal_details.Personal_Address;
											for each  other_addr in personal_details.Other_Addresses_for_shipment
											{
												if(other_addr.Preferred_address_for_delivery == true)
												{
													preferred_railway = other_addr.Nearest_Railway_Station_code;
													preferred_address = other_addr.Address;
													break;
												}
											}
											time_stamp = zoho.currenttime.toString();
											farm_rec = Farm_Details[Farm_ID = input.Farm_Details.Farm_ID];
											row1 = Kart.Order_Summery();
											row1.Cost_of_Purchase=contractor_rec.Amount;
											row1.Cost_per_unit=input.Cost_per_unit;
											row1.Quantity_of_purchase=contractor_rec.share;
											row1.Unit_of_Measurement=input.Unit_of_Measurement;
											row1.Product_Name=input.Crop.Crop_Name;
											rows = Collection();
											rows.insert(row1);
											kart_id = insert into Kart
										[
											Added_User=zoho.loginuser
											Kart_User_ID=contractor_rec.Added_User
											Order_Status="Order Accepted"
											Farm_Details=farm_rec
											Railwaystation_for_delivery=preferred_railway
											Address=preferred_address
											Order_Summery=rows
										];
											kart_list.add(kart_list);
										}
										contract_kart = insert into Contract_Kart
										[
											Added_User=zoho.loginuser
											Add_Contract=input.ID
											Contractor_Karts=kart_list
										];
										for each  harvest_item in input.Tentative_Harvest_Dates
										{
											if(harvest_item.Contract_Kart == null)
											{
												harvest_item.Contract_Kart=contract_kart;
												break;
											}
										}
					)
					}
				}
			}
