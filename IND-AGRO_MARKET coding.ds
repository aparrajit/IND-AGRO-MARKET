			on_validate9 as "on validate"
			{
				type =  form
				form = Become_a_contributor
				record event = on add

				on validate
				{
					actions 
					{
						custom deluge script
						(
											basic_contribution_percentage = (100 / input.Contract_Info.Maximum_Number_of_contributors).round(2);
										available_contribution_percentage = 100 - input.Contract_Info.Percentage_collected;
										if(!Contributing_more && !Self_funded)
										{
											if(available_contribution_percentage > basic_contribution_percentage)
											{
												min_contribution = basic_contribution_percentage;
											}
											else
											{
												min_contribution = available_contribution_percentage;
											}
											if(input.Percentage_of_Contribution < min_contribution)
											{
												alert "The minimum contribution percentage should be " + min_contribution + " %.";
												cancel submit;
											}
											else if(input.Percentage_of_Contribution > available_contribution_percentage)
											{
												alert "Maximum amount of contribution can be " + available_contribution_percentage + " %.";
												cancel submit;
											}
										}
										else if(input.Percentage_of_Contribution > available_contribution_percentage)
										{
											alert "Maximum amount of contribution can be " + available_contribution_percentage + " %.";
											cancel submit;
										}
						)
					}
				}

			}
