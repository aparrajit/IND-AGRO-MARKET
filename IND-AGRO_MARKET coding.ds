				 void createKartForContract(Tentative_Harvest_Dates ten_rec)
         {
         	ten_rec.Status="Completed";
         	if(ten_rec.Harvest_Investigator != null)
         	{
         		ten_rec.Investigator_Approval=true;
         	}
         	add_contract_rec = Add_Contract[ID == ten_rec.Add_Contract];
         	contract_kart = insert into Contract_Kart
         [
         	Added_User=zoho.loginuser
         	Add_Contract=ten_rec.Add_Contract
         	Tentative_Harvest_Dates_Number=ten_rec.ID
         ];
         	for each  contractor_rec in add_contract_rec.Contractor_Info
         	{
         		personal_details = Personal_Details[ID == contractor_rec.Personal_Details];
         		preferred_railway = personal_details.Nearest_RMS_station;
         		preferred_address = personal_details.Personal_Address;
         		for each  other_addr in personal_details.Other_Addresses_for_shipment
         		{
         			if(other_addr.Preferred_address_for_delivery == true)
         			{
         				preferred_railway = other_addr.Nearest_Railway_Station_code;
         				preferred_address = other_addr.Address;
         				break;
         			}
         		}
         		time_stamp = zoho.currenttime.toString();
         		farm_rec = Farm_Details[Farm_ID = add_contract_rec.Farm_Details.Farm_ID];
         		row1 = Kart.Order_Summery();
         		row1.Cost_of_Purchase=contractor_rec.Amount;
         		row1.Cost_per_unit=add_contract_rec.Cost_per_unit;
         		row1.Quantity_of_purchase=contractor_rec.share;
         		row1.Unit_of_Measurement=add_contract_rec.Unit_of_Measurement;
         		row1.Product_Name=add_contract_rec.Crop.Crop_Name;
         		rows = Collection();
         		rows.insert(row1);
         		kart_weight = ten_rec.Contract_Kart.Total_Weight * contractor_rec.Percent / 100;
         		kart_id = insert into Kart
         [
         	Added_User=zoho.loginuser
         	Kart_User_ID=contractor_rec.Added_User
         	Order_Status="Order Accepted"
         	Farm_Details=farm_rec.ID
         	Railwaystation_for_delivery=preferred_railway
         	Address=preferred_address
         	ready_for_packing=true
         	Order_Summery=rows
         	Contract_Kart=contract_kart
         	Approximate_weight=kart_weight
         	Contract_Order=true
         ];
         	}
         	Status = "Completed";
         	next_harvest_date = Tentative_Harvest_Dates[Add_Contract == ten_rec.Add_Contract && Number == ten_rec.Number + 1];
         	if(next_harvest_date.count() > 0)
         	{
         		next_harvest_date.Status="Ongoing";
         	}
         }
