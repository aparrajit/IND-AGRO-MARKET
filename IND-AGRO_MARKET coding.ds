				 collection string_to_address(string add_str)
         {
         	x = Collection();
         	split_string = add_str.toList(",");
         	list_lenght = split_string.size();
         	x_string = "";
         	iterationString = "".leftPad(4).replaceAll(" ",",").toList().subList(0,4);
         	i = 0;
         	for each  i_x in iterationString
         	{
         		if(i == 0)
         		{
         			x.insert("country":split_string.get(list_lenght - 1).toString());
         		}
         		else if(i == 1)
         		{
         			x.insert("postal_Code":split_string.get(list_lenght - 2).toString());
         		}
         		else if(i == 2)
         		{
         			x.insert("state_province":split_string.get(list_lenght - 3).toString());
         		}
         		else if(i == 3)
         		{
         			x.insert("district_city":split_string.get(list_lenght - 4).toString());
         		}
         		i = i + 1;
         	}
         	x.insert("address_line_1":split_string.subList(0,list_lenght - 4));
         	return x;
         }
