										{
											if(Pickup_on.isEmpty())
											{
												alert "Pickup time cannot be empty";
												error = true;
											}
											else if(Pickup_on < zoho.currentdate)
											{
												alert "Pickup time cannot be less than today";
												error = true;
											}
											else
											{
												pick_up_time = input.Pickup_on.toString();
											}
										}
										else
										{
											pick_up_time = zoho.currenttime;
										}
										if(!error)
										{
											farm_rec = Farm_Details[ID == input.Kart.Farm_Details];
											if(input.Pickup_From == "Farm")
											{
												pick_up_addr = input.Kart.Farm_Details.Farm_Address;
