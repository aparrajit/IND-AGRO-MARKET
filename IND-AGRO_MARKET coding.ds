			buy_with_community as "buy with community"
			{
				type =  functions
				form = Combo_Pack
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						per_rec = Personal_Details[Added_User == zoho.loginuser];
										if(per_rec.count() == 0)
										{
											openUrl("#Form:Personal_Details?From_error=true","new window");
										}
										else
										{
											my_communities = Community[Community_Members.User_ID == zoho.loginuser];
											my_community_carts = Community_Kart[Community in my_communities && Products1.contains(input.ID) && Status == "Fund Collection" || Status == "Order Placed"];
											if(my_community_carts.count() > 0)
											{
												openUrl("#Report:My_Community_Karts?ID=" + my_community_carts.ID,"new window");
											}
											else
											{
												product_ids_list = Products[Combo_Pack == input.ID].ID.getAll();
												openUrl("#Form:Community_Kart?Products1=" + product_ids_list + "&Cost_of_packaging=" + input.Packaging_Price,"new window");
											}
										}
					)
					}
				}
			}
