			on_validate1 as "on validate"
			{
				type =  form
				form = Add_Contract
				record event = on add

				on validate
				{
					actions 
					{
						custom deluge script
						(
											farm_rec = Farm_Details[Added_User == zoho.loginuser];
										d_c = farm_rec.Farm_Address.district_city;
										allowed_districts = Contract_Districts[Name.equalsIgnoreCase(d_c)];
										if(allowed_districts.count() == 0)
										{
											alert "Contract serives are allowed only in few districts. Your district will be soon added to this , as we grow";
											cancel submit;
										}
										if(Unit_of_Measurement == "Acres" && Total_Units > input.Farm_Details.Farming_area)
										{
											alert "Your Farming area given in Farm Details is less than Contract area.";
											cancel submit;
										}
										if(zoho.currentdate.daysBetween(input.Tentaive_Farming_Start) < 7)
										{
											alert "Tentative farming start date should be atleast 7 days from today.";
											cancel submit;
										}
										previous_date = input.Tentaive_Farming_Start;
										flag = false;
										for each  x in input.Tentative_Harvest_Dates
										{
											if(x.Harvest_Date <= previous_date)
											{
												alert "Tentative harvest dates should be in increasing order.";
												flag = true;
												cancel submit;
												//break;
											}
											else if(x.Harvest_Date > input.Contract_End_Date)
											{
												alert "One of the harvest dates is greater than Contract end date";
												flag = true;
												cancel submit;
												//break;
											}
											if(flag)
											{
												break;
											}
											previous_date = x.Harvest_Date;
										}
										openContracts = Add_Contract[Added_User == zoho.loginuser && Contract_Status == "Fund Collection"];
										if(openContracts.count() == 5)
										{
											alert "There can be only 5 open contracts. You have exceeded the limit";
											cancel submit;
										}
										if(Contract_By_Aucction == "Yes")
										{
											if(Auction_end_date.isEmpty())
											{
												alert "Auction end date cannot be empty.";
												cancel submit;
											}
											else if(Auction_end_date <= zoho.currentdate)
											{
												alert "Auction end date should not be today or less than today.";
												cancel submit;
											}
											else if(zoho.currentdate.daysBetween(input.Auction_end_date) > 30)
											{
												alert "Maximum days of auction is 30 days. Please change the auction date to " + zoho.currentdate.addDay(30);
												cancel submit;
											}
											else if(Auction_end_date > Tentaive_Farming_Start)
											{
												alert "Auction end date should before Tentative Farming Start date";
												cancel submit;
											}
											else if(Auction_end_date.daysBetween(input.Tentaive_Farming_Start) < 3)
											{
												alert "Auction end date should be atleast 3 days prior to Tentative Farming Start date";
												cancel submit;
											}
										}
						)
					}
				}

			}
