				 bool hasPossiblePhoneNumberOrPinCode(string str)
         {
         	all_caps = str.toUpperCase();
         	specail_char_list = {" ","_","(",")","!","@","#","$","%","^","&","*","-","+","=",",","<",".",">","?","/","\"","'",":",";","{","[","]","}","|","\\","`","~"};
         	for each  rec in specail_char_list
         	{
         		all_caps = all_caps.remove(rec);
         	}
         	just_numbers_string = all_caps.removeAllAlpha();
         	num = just_numbers_string.length();
         	denominatons = {"ONE","TWO","THREE","FOUR","FIVE","SIX","SEVEN","EIGHT","NINE","TEN","ELEVEN","TWELVE","TEEN","TY","HUNDERED","THOUSAND","LAKH","CRORE","MILLION","BILLION","TRILLION"};
         	word_len = all_caps.length();
         	for each  d_rec in denominatons
         	{
         		d_len = d_rec.length();
         		x = all_caps.remove(d_rec);
         		difference = word_len - x.len();
         		num_length = difference / d_len;
         		num = num + num_length;
         	}
         	return num > 5;
         }
