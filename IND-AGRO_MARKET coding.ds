			accept_order1 as "accept order"
			{
				type =  functions
				form = Community_Kart
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						input.Status = "Order Accepted";
										payed_orders = Kart[Community_Kart == input.ID && Order_Status == "Partial Order"];
										for each  x in payed_orders
										{
